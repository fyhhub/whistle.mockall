{
  "name": "whistle.mockall",
  "version": "0.0.3",
  "description": "Mock All",
  "homepage": "https://github.com/fyhhub/whistle.mockall",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --config ./app/vite.config.ts -w",
    "dev:w2": "npm-run-all --parallel server:run server:lack server:watch",
    "server:watch": "tsc -w",
    "server:run": "npx w2 run",
    "server:lack": "npx lack watch"
  },
  "files": [
    "app",
    "dist",
    "public",
    "index.js"
  ],
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.5",
    "vue-tsc": "^1.2.1"
  },
  "whistleConfig": {
    "inspectorTabs": {
      "page": "public/res.html",
      "name": "Mock All",
      "res": {
        "action": "public/res.html",
        "name": "Replace"
      },
      "req": {
        "action": "public/req.html",
        "name": "Replace"
      }
    }
  },
  "dependencies": {
    "js-base64": "^3.7.5",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lite-json-editor": "^0.1.3",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "simple-code-editor": "^2.0.8",
    "vue": "^3.3.4"
  }
}
